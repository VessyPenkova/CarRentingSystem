﻿@using CarRentingSystem.Areas.Admin.Models 
@model MyCarRoutesViewModel
@{
    ViewBag.Title = "My CarRoutes";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<h4>My Added CarRoutes</h4>

@if (!Model.AddedCarRoutes.Any())
{
    <h2 class="text-center">You have no added carRouteses yet!</h2>
}

<div class="row">
    @foreach (var carRoute in Model.AddedCarRoutes)
    {
        @await Html.PartialAsync("~/Views/Shared/_CarRoutePartial.cshtml", carRoute)
    }
</div>

<h4>My Rented CarRouteses</h4>

@if (!Model.RentedCarRoutes.Any())
{
    <h2 class="text-center">You have no rented carRouteses yet!</h2>
}

<div class="row">
    @foreach (var carRoute in Model.RentedCarRoutes)
    {
        @await Html.PartialAsync("~/Views/Shared/_CarRoutePartial.cshtml", carRoute)
    }
</div>