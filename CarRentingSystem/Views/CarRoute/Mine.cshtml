@using CarRentingSystem.Core.Models.CarRoute﻿

@model IEnumerable<CarRouteServiceModel>
@{
    ViewBag.title = "My CarRoutes";
}
<h2 class="text-center">@ViewBag.Title</h2>
<hr />


@if (Model.Any())
{
    <div class="row">
        @foreach (var carRoute in Model)
        {
            <partial name="_CarRoutePartial" model="@carRoute" />
        }
    </div>
}
else
{
    <h3 class="text-center">You have no car or routes yet!</h3>
}